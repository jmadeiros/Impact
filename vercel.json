{
  "version": 2,
  "name": "rag-survey-system",
  "builds": [
    {
      "src": "vercel-deployment/api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/vercel-deployment/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel-deployment/api/health"
    }
  ],
  "env": {
    "PYTHONPATH": "./vercel-deployment/lib"
  }
}